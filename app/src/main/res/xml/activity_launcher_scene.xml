<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        android:id="@+id/launch"
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/start"
        motion:constraintSetStart="@id/init"
        motion:duration="1000">
        <KeyFrameSet>
            <KeyAttribute
                motion:motionTarget="@id/oval"
                motion:framePosition="0"
                android:scaleX="1.5"
                android:scaleY="1.5"
                />
            <KeyAttribute
                motion:motionTarget="@id/triangle"
                motion:framePosition="0"
                android:scaleX="1.5"
                android:scaleY="1.5"
                />
            <KeyAttribute
                motion:motionTarget="@id/rectangle"
                motion:framePosition="0"
                android:scaleX="1.5"
                android:scaleY="1.5"
                />
        </KeyFrameSet>
    </Transition>
    <Transition
        android:id="@+id/begin"
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/mid"
        motion:constraintSetStart="@id/start"
        motion:duration="2000">
    </Transition>

    <Transition
        android:id="@+id/fin"
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/mid"
        motion:duration="2000">
    </Transition>

    <ConstraintSet android:id="@+id/init">
        <Constraint
            android:id="@+id/oval"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintDimensionRatio="w, 1:1"
            motion:layout_constraintEnd_toStartOf="@id/triangle"
            motion:layout_constraintHorizontal_chainStyle="packed"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintWidth_percent=".1"
            android:alpha="0.2"
            />
        <Constraint
            android:id="@+id/triangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintEnd_toStartOf="@id/rectangle"
            motion:layout_constraintStart_toEndOf="@id/oval"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintWidth_percent=".11"
            android:alpha="0.2"
            />
        <Constraint
            android:id="@+id/rectangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/triangle"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintWidth_percent=".1"
            android:alpha="0.2"
            />

        <Constraint
            android:id="@+id/letter0"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintDimensionRatio="w,1"
            motion:layout_constraintStart_toStartOf="@id/guideline_start"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter1"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            motion:layout_constraintStart_toEndOf="@id/letter0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter2"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter3"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toEndOf="@id/guideline_end"
            motion:layout_constraintStart_toEndOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/letter1"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter3_triangle"
            motion:layout_constraintWidth_percent=".11"
            motion:layout_constraintEnd_toEndOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="@id/guideline_bottom"
            motion:layout_constraintStart_toEndOf="@id/letter2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias=".05"
            motion:layout_constraintHorizontal_bias=".2"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/oval"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintDimensionRatio="w, 1:1"
            motion:layout_constraintEnd_toStartOf="@id/triangle"
            motion:layout_constraintHorizontal_chainStyle="packed"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintWidth_percent=".1" />
        <Constraint
            android:id="@+id/triangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintEnd_toStartOf="@id/rectangle"
            motion:layout_constraintStart_toEndOf="@id/oval"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintWidth_percent=".11" />
        <Constraint
            android:id="@+id/rectangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/triangle"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintWidth_percent=".1" />

        <Constraint
            android:id="@+id/letter0"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintDimensionRatio="w,1"
            motion:layout_constraintStart_toStartOf="@id/guideline_start"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter1"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            motion:layout_constraintStart_toEndOf="@id/letter0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter2"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter3"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toEndOf="@id/guideline_end"
            motion:layout_constraintStart_toEndOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/letter1"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter3_triangle"
            motion:layout_constraintWidth_percent=".11"
            motion:layout_constraintEnd_toEndOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="@id/guideline_bottom"
            motion:layout_constraintStart_toEndOf="@id/letter2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias=".05"
            motion:layout_constraintHorizontal_bias=".2"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/mid">
        <Constraint
            android:id="@+id/oval"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w, 1:1"
            motion:layout_constraintEnd_toStartOf="@id/guideline_start"
            motion:layout_constraintWidth_percent=".1" />
        <Constraint
            android:id="@+id/triangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_top"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintStart_toStartOf="@id/letter1"
            motion:layout_constraintWidth_percent=".12" />
        <Constraint
            android:id="@+id/rectangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/letter2"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintEnd_toEndOf="@id/letter2"
            motion:layout_constraintWidth_percent=".1" />
        <Constraint
            android:id="@+id/letter0"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintDimensionRatio="w,1"
            motion:layout_constraintStart_toStartOf="@id/guideline_start"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter1"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            motion:layout_constraintStart_toEndOf="@id/letter0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter2"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter3"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toEndOf="@id/guideline_end"
            motion:layout_constraintStart_toEndOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/letter1"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
        <Constraint
            android:id="@+id/letter3_triangle"
            motion:layout_constraintWidth_percent=".11"
            motion:layout_constraintEnd_toEndOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="@id/guideline_bottom"
            motion:layout_constraintStart_toEndOf="@id/letter2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias=".05"
            motion:layout_constraintHorizontal_bias=".2"
            motion:layout_constraintDimensionRatio="w,1"
            android:alpha="0"
            />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/oval"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w, 1:1"
            motion:layout_constraintEnd_toStartOf="@id/guideline_start"
            motion:layout_constraintWidth_percent=".1" />
        <Constraint
            android:id="@+id/triangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_top"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintStart_toStartOf="@id/letter1"
            motion:layout_constraintWidth_percent=".12" />
        <Constraint
            android:id="@+id/rectangle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/letter2"
            motion:layout_constraintDimensionRatio="w,1:1"
            motion:layout_constraintEnd_toEndOf="@id/letter2"
            motion:layout_constraintWidth_percent=".1" />
        <Constraint
            android:id="@+id/letter0"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintDimensionRatio="w,1"
            motion:layout_constraintStart_toStartOf="@id/guideline_start" />
        <Constraint
            android:id="@+id/letter1"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            motion:layout_constraintStart_toEndOf="@id/letter0"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1" />
        <Constraint
            android:id="@+id/letter2"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toStartOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/guideline_bottom"
            motion:layout_constraintDimensionRatio="w,1" />
        <Constraint
            android:id="@+id/letter3"
            motion:layout_constraintWidth_percent=".24"
            motion:layout_constraintEnd_toEndOf="@id/guideline_end"
            motion:layout_constraintStart_toEndOf="@id/letter1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toBottomOf="@id/letter1"
            motion:layout_constraintDimensionRatio="w,1" />
        <Constraint
            android:id="@+id/letter3_triangle"
            motion:layout_constraintWidth_percent=".11"
            motion:layout_constraintEnd_toEndOf="@id/letter3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="@id/guideline_bottom"
            motion:layout_constraintStart_toEndOf="@id/letter2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias=".05"
            motion:layout_constraintHorizontal_bias=".2"
            motion:layout_constraintDimensionRatio="w,1" />
    </ConstraintSet>
</MotionScene>